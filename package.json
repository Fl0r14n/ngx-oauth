    "tslint": "*",
    "codelyzer": "*",
    "ng-packagr": "*",
    "rimraf": "*"
  },
  "dependencies": {
    "@angular/common": "*",
    "@angular/core": "*",
    "@angular/forms": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
